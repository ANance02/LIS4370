# Sorting by Top Teams
top_fielding_teams <- function(stat, n = 5) {
data <- load_fielding_data()
stat <- trimws(stat)
if (!(stat %in% names(data))) {
stop(paste("Column", stat, "not found in the dataset."))
}
if (!is.numeric(data[[stat]])) {
stop(paste("Column", stat, "is not numeric. It is:", class(data[[stat]])))
}
data <- data[order(-data[[stat]]), ]
return(data[1:min(n, nrow(data)), c("Team", stat)])
}
